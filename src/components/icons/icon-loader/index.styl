@import '../../../mixin'

.icon-loader
  position absolute
  top calc( 50% - 20px )
  left calc( 50% - 20px )

  &__svg
    width 40px
    height 40px
    margin auto 
    display block 
    shape-rendering auto
    
  &__circle
    stroke grey 
    stroke-width 10px
    transform rotate(269.782 50 50) 

